<?xml version="1.0" encoding="utf-8"?>
<web-app
        version="2.5"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
       <servlet-name>passbook</servlet-name>
      <servlet-class>br.com.sas.PassbookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>passbook</servlet-name>
      <url-pattern>/passbook/saude</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>passbookCard</servlet-name>
        <servlet-class>br.com.sas.Passbook2Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>passbookCard</servlet-name>
        <url-pattern>/passbook/saude/2</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>saude</servlet-name>
        <jsp-file>saude.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>saude</servlet-name>
        <url-pattern>/saude</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>guestbook.jsp</welcome-file>
    </welcome-file-list>

</web-app>
